package gridLayout;

import java.awt.*;
import javax.swing.JButton;
import javax.swing.JFrame;

public class Layout extends JFrame {
	setLayout(new GridLayout(3,2));
	add(new Button("1"));
    add(new Button("2"));
    add(new Button("3"));
    add(new Button("4"));
    add(new Button("5"));
    add(new Button("6"));
}